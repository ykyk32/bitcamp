package multi;

public class Zzz {

	public static void main(String[] args) {
		
		int[][] ar = new int[3][];
		ar[0]=new int[2];
		ar[1]=new int[3];
		ar[2]=new int[4];
		
		int sum= 0;
		for(int i = 0;i<ar.length;i++) {
			for(int j=0;j<ar[i].length;j++) {
				sum++;
				ar[i][j]=sum;
			
			}
		}
		
		for(int i=0;i<ar.length;i++) {
			for(int j=0; j<ar[i].length;j++) {
				System.out.println("ar["+i+"]["+j+"] = "+ar[i][j]);
			}System.out.println();
		}
		

	}

}

