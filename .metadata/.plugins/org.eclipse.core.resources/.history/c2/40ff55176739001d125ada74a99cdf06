package array;

import java.util.Random;
public class Lotto {

	public static void main(String[] args) {
		int[] lotto = new int[6];
			
		Random r = new Random();
		
		for(int i=0;i<lotto.length;i++) {
			for(int j=1;j<lotto.length;j++)
				lotto[i] = r.nextInt(45)+1;
			if () {}
		}
	}

}


/*
-6개의 숫자가 1~45 사이에서 나온다
-중복 안됨
*/