package class_;


class Person{
	String name;
	int age;
	

}
//-------------------
public class PersonMain {

	public static void main(String[] args) {
		Person aa;;//클래스 변수=>객체
		aa= new Person();//메모리에 생성
		System.out.println("객체 aa=" + aa);//.Person@6f2b958e

		aa.name = "홍길동";//aa안에있는 name에 준다
		aa.age=25;
		System.out.println("이름 = "+ aa.name+ "\t 나이 = "+ aa.age);
	}

}
