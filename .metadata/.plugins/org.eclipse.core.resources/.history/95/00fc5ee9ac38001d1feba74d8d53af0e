package array;

public class Array01 {

	public static void main(String[] args) {
		int a;//정수형 변수
		int[] ar;//정수형 배열,배열을 선언
		ar = new int[5];//생성
		ar[0]=25;
		ar[1]=78;
		ar[2]=30;
		ar[3]=43;
		ar[4]=52;
		
		
		System.out.println("배열명 ar =" + ar);
		System.out.println("베열 크기 ="+ar.length);
					//limit정할때 배열의 크기와 맞추는게 좋음
		for(int i=0;i<ar.length;i++) {
			System.out.println("ar["+i+"] = "+ar[i]);
		}
		
	}

}
