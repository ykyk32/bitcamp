package multi;

public class MultiArray02 {

	public static void main(String[] args) {
		int[][] ar = new int[10][10];
		int num=0;
		
		//입력
//		for(int i =0;i<ar.length;i++) {
//			for(int j=0;j<ar[i].length;j++) {
//				num++;//1
//				ar[i][j]=num;//1
//		
//			}

//		}
		
		//입력2
//		for(int i =0;i<ar.length;i++) {
//			for(int j=0;j<ar[i].length;j++) {
//				num++;//1
//				ar[j][i]=num;//1
//			}
//		}
		
		//입력3
		int num2=100;
		for(int i =0;i<ar.length;i++) {
			for(int j=0;j<ar[i].length;j++) {
				
				ar[i][j]=num2;
				num2--;
			}
		}
			
			
		
		
		
		
		//출력
		for(int i =0;i<ar.length;i++) {
			for(int j=0;j<ar[i].length;j++) {
				
			
				System.out.print(String.format("%4d", ar[i][j]));
			}System.out.println();
		}
		
	}

}
