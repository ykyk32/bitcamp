package for_;

public class MultiFor02 {

	public static void main(String[] args) {
		int dan, i;
		
		for(dan = 1;dan <=9;dan++) {
			for(i=1;i<=9;i++) {
				System.out.println(dan + "*" + i + "=" + dan*i);
			}System.out.println();
		}
		
	}

}

/*
 2단 ~ 9단 구구단
 
 2*1=2
 2*2=4
 2*9=18
 
 
 3*1=3
 3*2=6
 3*9=27
 */