package constructor;


class Hello{
	private String name;
	private int age;
	
	public Hello() {
		System.out.println("기본 생성자");
	}
	public Hello(String name) {
		this.name=name;
	}
	public String getName() {
		return name;
	}
}


//----------------------------
public class ConstructorMain {

	public static void main(String[] args) {
		Hello aa = new Hello();//>생성자 호출
		System.out.println(aa.getName());
	}

}
