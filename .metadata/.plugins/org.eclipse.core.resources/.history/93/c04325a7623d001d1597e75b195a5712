package inheritence;

import java.util.Scanner;

class ShapeTest{
	protected double area;
	protected Scanner scan = new Scanner(System.in);
	
	public ShapeTest() {
		System.out.println("ShapeTest 기본생성자");
		
	}
	public void calcArea() {
		System.out.println("도형을 계산합니다");
		
	}
	public void dispArea() {
		System.out.println("도형을 계산합니다");
		
	}
}

